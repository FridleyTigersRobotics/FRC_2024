{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "numbers": "cpp",
    "string": "cpp",
    "memory_resource": "cpp",
    "limits": "cpp",
    "memory": "cpp",
    "mutex": "cpp",
    "sstream": "cpp",
    "functional": "cpp",
    "utility": "cpp",
    "*.inc": "cpp"
  }
}
